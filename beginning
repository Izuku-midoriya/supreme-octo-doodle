""" A Functional Breakfast """
def make_omlette():
  print('Mixing the ingredients')
  print('Pouring the mixture into a frying pan')
  print('Cooking the first side')
  print('Flipping it!')
  print('Cooking the other side')
  omelette = 'a tasty omelette'
  return omlette
  
  omlette1 = make_omlette()
